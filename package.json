    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.3",
    "helmet": "^4.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.3.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
